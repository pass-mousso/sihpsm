            {% for section in _menu %}
                <div class="menu-item pt-5">
                    <div class="menu-content">
                        <span class="menu-heading fw-bold text-uppercase fs-7">{{ section.title }}</span>
                    </div>
                </div>
                {% for item in section.menus %}
                    <div data-kt-menu-trigger="click" class="menu-item menu-accordion">
                        <span class="menu-link">
                            <span class="menu-icon">{{ theme.getIcon(item.icon, 'fs-2') | raw }}</span>
                            <span class="menu-title">{{ item.label }}</span>
                            <span class="menu-arrow"></span>
                        </span>
                        {% if item.submenus is not empty %}
                            <div class="menu-sub menu-sub-accordion">
                                {% for subitem in item.submenus %}
                                    <div class="menu-item">
                                        <a class="menu-link" href="{{ path(subitem.route ?? '#') }}">
                                            <span class="menu-bullet">
                                                <span class="bullet bullet-dot"></span>
                                            </span>
                                            <span class="menu-title">{{ subitem.label }}</span>
                                        </a>
                                    </div>
